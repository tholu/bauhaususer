	"keywords": ["bauhaus", "krafthaus", "user", "users", "login", "backend"],
	"type": "laravel-package",
	"license": "MIT",
	"authors": [
		{
			"name": "KraftHaus",
			"email": "hello@krafthaus.nl"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"krafthaus/bauhaus": "*",
	  	"cartalyst/sentry": "~2.1"
	},
	"autoload": {
		"classmap": [
			"src/controllers",
			"src/models",
			"src/admin",
			"src/seeds",
			"src/commands"
